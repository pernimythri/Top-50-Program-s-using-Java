import java.io.*;   // Required for input/output operations
import java.util.*;   // Provides classes
class practice
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in); 
        int t=sc.nextInt();   //No.of test cases
        for(int i=0;i<t;i++)
        {
            String str=sc.next(); 
            String nstr="";
            char ch;
            for(int j=0;j<str.length();j++)
            {
                nstr=str.charAt(j)+nstr;  //charAt used to get specific   index position  character in string
        }
        System.out.println("reverse: "+ nstr);
    }
    }
}
